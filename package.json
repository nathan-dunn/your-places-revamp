{
  "name": "your-places-revamp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "setup": "node ./scripts/setup.js",
    "build": "vite build",
    "dev": "vite",
    "start:dev": "NODE_ENV=development nodemon ./src/back/index.js",
    "db:dev": "NODE_ENV=development mongod",
    "heroku": "git push -f heroku HEAD:master",
    "surge": "npm run build && surge --project ./dist --domain https://yourplaces.surge.sh",
    "deploy": "npm run heroku && npm run surge"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "espress": "^0.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^6.1.0",
    "mongoose": "^7.6.1",
    "morgan": "^1.10.0",
    "normalize-css": "^2.3.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "sass": "^1.69.2",
    "simple-line-icons": "^2.5.5",
    "tachyons": "^4.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdoc": "^4.0.2",
    "prettier": "^3.0.3",
    "vite": "^4.4.5"
  }
}
